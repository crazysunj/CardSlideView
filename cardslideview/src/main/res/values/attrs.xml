<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="card_item_margin_percent" format="float" />
    <attr name="card_loop" format="boolean" />
    <attr name="card_rebound" format="boolean" />
    <attr name="card_side_offset_percent" format="float" />
    <attr name="card_item_rate" format="float" />
    <attr name="card_page_limit" format="integer" />
    <attr name="card_page_mode" format="enum">
        <enum name="linear" value="0" />
        <enum name="page" value="1" />
    </attr>

    <declare-styleable name="CardSlideView">
        <attr name="card_item_margin_percent" />
        <attr name="card_loop" />
        <attr name="card_side_offset_percent" />
        <attr name="card_item_rate" />
        <attr name="android:orientation" />
        <attr name="card_page_mode" />
        <attr name="card_rebound" />
        <attr name="card_page_limit" />
    </declare-styleable>
</resources>